.panel.panel-primary
   .panel-heading.text-center New Course
   .panel-body
      .row(ng-repeat="course in main.courses | orderBy: 'published' :true | limitTo: 10")
         .col-md-3 {{course.published | date: 'MM/dd/yy'}}
         .col-md-9 {{course.name}}
