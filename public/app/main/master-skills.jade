.panel.panel-primary
    .panel-heading.text-center {{'body.skills.masterSkills' | translate}}
    .panel-body
        .row(ng-repeat="skill in main.skills | filter:{mastered: true}")
            .col-md-3 {{::skill.published | date: 'MM/yyyy'}}
            .col-md-9
                a(ui-sref="app.skills.skill-detail({id: skill._id})") {{skill.title}}
